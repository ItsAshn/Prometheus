{
  "name": "my-qwik-empty-starter",
  "version": "1.0.0",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/ensure-jwt.js",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "predev": "node scripts/ensure-jwt.js",
    "dev": "node --max-old-space-size=8192 --max-http-header-size=81920 ./node_modules/vite/bin/vite.js --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "cleanup": "node scripts/cleanup.js",
    "setup": "node scripts/setup-env.js",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "preserve": "node scripts/ensure-jwt.js",
    "serve": "node server/entry.express",
    "prestart": "node scripts/ensure-jwt.js",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "docker:build": "docker build -t itsashn/prometheus:latest .",
    "docker:build:version": "docker build --build-arg APP_VERSION=%npm_package_version% --build-arg BUILD_DATE=%date:~-4,4%-%date:~-10,2%-%date:~-7,2%T%time:~0,2%:%time:~3,2%:%time:~6,2% -t itsashn/prometheus:%npm_package_version% -t itsashn/prometheus:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:update": "bash scripts/update-docker.sh",
    "docker:update:win": "powershell -ExecutionPolicy Bypass -File scripts/update-docker.ps1",
    "docker:push": "docker push itsashn/prometheus:latest",
    "docker:push:version": "docker push itsashn/prometheus:%npm_package_version% && docker push itsashn/prometheus:latest"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.16.0",
    "@builder.io/qwik-city": "^1.16.0",
    "@eslint/js": "latest",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.19",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.19.0",
    "@types/uuid": "^11.0.0",
    "eslint": "9.32.0",
    "eslint-plugin-qwik": "^1.16.0",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "typescript": "5.4.5",
    "typescript-eslint": "8.38.0",
    "typescript-plugin-css-modules": "latest",
    "vite": "7.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cookie": "^1.0.2",
    "dotenv": "^16.3.2",
    "express": "4.20.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "undici": "*",
    "uuid": "^13.0.0"
  }
}
